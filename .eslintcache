 (uv.y < 0.0) { uv.y = 0.0; multiplier.y = -1.0; };\n  if (uv.y > 1.0) { uv.y = 1.0; multiplier.y = -1.0; };\n  return multiplier * texture2D(uVelocity, uv).xy;\n}\n\nvoid main () {\n  float L = sampleVelocity(vL).x;\n  float R = sampleVelocity(vR).x;\n  float T = sampleVelocity(vT).y;\n  float B = sampleVelocity(vB).y;\n  float div = 0.5 * (R - L + T - B);\n  gl_FragColor = vec4(div, 0.0, 0.0, 1.0);\n}\n`\n","/Users/sofmen/Desktop/sanskar/mostDone/client/src/components/fluidAnimation/react-fluid-animation/shaders/curl.js",["582"],"/Users/sofmen/Desktop/sanskar/mostDone/client/src/pages/IntroContent.js",[],"/Users/sofmen/Desktop/sanskar/mostDone/client/src/components/parallexComponent/NewsCards.js",[],"/Users/sofmen/Desktop/sanskar/mostDone/client/src/components/parallexComponent/StaffCollection.js",[],"/Users/sofmen/Desktop/sanskar/mostDone/client/src/components/parallexComponent/BigTextMobile.js",[],"/Users/sofmen/Desktop/sanskar/mostDone/client/src/components/fluidAnimation/react-fluid-animation/react-fluid-animation.js",[],"/Users/sofmen/Desktop/sanskar/mostDone/client/src/components/fluidAnimation/react-fluid-animation/get-gl-context.js",[],"/Users/sofmen/Desktop/sanskar/mostDone/client/src/components/fluidAnimation/react-fluid-animation/shaders/advection-manual-filtering.js",["583"],"export default `\nprecision highp float;\nprecision mediump sampler2D;\n\nvarying vec2 vUv;\nuniform sampler2D uVelocity;\nuniform sampler2D uSource;\nuniform vec2 texelSize;\nuniform float dt;\nuniform float dissipation;\n\nvec4 bilerp (in sampler2D sam, in vec2 p) {\n  vec4 st;\n  st.xy = floor(p - 0.5) + 0.5;\n  st.zw = st.xy + 1.0;\n  vec4 uv = st * texelSize.xyxy;\n  vec4 a = texture2D(sam, uv.xy);\n  vec4 b = texture2D(sam, uv.zy);\n  vec4 c = texture2D(sam, uv.xw);\n  vec4 d = texture2D(sam, uv.zw);\n  vec2 f = p - st.xy;\n  return mix(mix(a, b, f.x), mix(c, d, f.x), f.y);\n}\n\nvoid main () {\n  vec2 coord = gl_FragCoord.xy - dt * texture2D(uVelocity, vUv).xy;\n  gl_FragColor = dissipation * bilerp(uSource, coord);\n  gl_FragColor.a = 1.0;\n}\n`\n","/Users/sofmen/Desktop/sanskar/mostDone/client/src/components/fluidAnimation/react-fluid-animation/shaders/display.js",["584"],"/Users/sofmen/Desktop/sanskar/mostDone/client/src/components/fluidAnimation/react-fluid-animation/shaders/vert.js",["585"],"/Users/sofmen/Desktop/sanskar/mostDone/client/src/components/fluidAnimation/react-fluid-animation/shaders/pressure.js",["586"],"/Users/sofmen/Desktop/sanskar/mostDone/client/src/components/parallexComponent/ToutHeader.js",[],"/Users/sofmen/Desktop/sanskar/mostDone/client/src/components/fluidAnimation/react-fluid-animation/shaders/index.js",["587"],"/Users/sofmen/Desktop/sanskar/mostDone/client/src/components/fluidAnimation/react-fluid-animation/gl-program.js",[],{"ruleId":"588","severity":1,"message":"589","line":3,"column":8,"nodeType":"590","messageId":"591","endLine":3,"endColumn":14},{"ruleId":"588","severity":1,"message":"592","line":3,"column":18,"nodeType":"590","messageId":"591","endLine":3,"endColumn":27},{"ruleId":"588","severity":1,"message":"593","line":3,"column":29,"nodeType":"590","messageId":"591","endLine":3,"endColumn":38},{"ruleId":"588","severity":1,"message":"594","line":3,"column":40,"nodeType":"590","messageId":"591","endLine":3,"endColumn":49},{"ruleId":"588","severity":1,"message":"595","line":4,"column":9,"nodeType":"590","messageId":"591","endLine":4,"endColumn":14},{"ruleId":"588","severity":1,"message":"596","line":4,"column":15,"nodeType":"590","messageId":"591","endLine":4,"endColumn":21},{"ruleId":"597","replacedBy":"598"},{"ruleId":"599","replacedBy":"600"},{"ruleId":"597","replacedBy":"601"},{"ruleId":"599","replacedBy":"602"},{"ruleId":"588","severity":1,"message":"603","line":9,"column":8,"nodeType":"590","messageId":"591","endLine":9,"endColumn":17},{"ruleId":"588","severity":1,"message":"604","line":13,"column":8,"nodeType":"590","messageId":"591","endLine":13,"endColumn":15},{"ruleId":"588","severity":1,